<?xml version="1.0" encoding="UTF-8"?>
<Configuration>
    <!--<Properties>-->
        <!--<Property></Property>-->
    <!--</Properties>-->
    <!--<ThresholdFilter/>-->
    <Appenders>
        <Console name="Console" target="SYSTEM_OUT">
            <PatternLayout pattern="%d{HH:mm:ss.SSS} [%t] %-5level %logger{36} -%msg%n"/>
        </Console>
    </Appenders>
    <Loggers>
        <root level="trace">
            <appender-ref ref="Console"/>
        </root>
    </Loggers>

</Configuration>